printf("The Environment\n\n");
auto k;
forall (k in sort(keys(env)))
     printf("%s=%s\n", k, env[k]);

printf("\nSetting\n");
printf("Current FOO == %s\n", string(env.FOO));
env.FOO = "Bar";
printf("Following env.FOO = Bar\nFOO == %s\n", string(env.FOO));

printf("\nRunning echo in a shell\n");
flush();
system("echo $FOO");
