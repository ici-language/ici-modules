CFLAGS	= $(PIC) $(OPTIM) -I. -I$(PREFIX)/include -I. $(ICICFLAGS)
OBJS	= $(ICIOBJS)
LIB	= $(ICIV)$(MOD).$(SOSUFFIX)
LIBS	= -lsocket -lnsl
$(OTHERDEFS)

$(LIB): $(OBJS)
	$(CC) $(ICILDFLAGS) -o $@ $(OBJS) $(LIBS)

clean:
	rm -f $(LIB) $(OBJS)

clobber: clean
	rm -f .depend

depend:
	mkdep $(CFLAGS) $(ICISRCS)

install: $(LIBINSTALLS)
	$(INSTALL) $(LIBINSTALLS) $(PREFIX)/lib/ici4

$(OTHERRULES)
