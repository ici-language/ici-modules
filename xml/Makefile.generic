CFLAGS	= $(PIC) $(OPTIM) -I. -I$(PREFIX)/include $(ICICFLAGS) -Iexpat/xmlparse -Iexpat/xmltok
OBJS	= xml.o xmlparse.o xmltok.o xmlrole.o
LIB	= $(ICIV)$(MOD).$(SOSUFFIX)
$(OTHERDEFS)

$(LIB): $(OBJS)
	$(CC) $(ICILDFLAGS) -o $@ $(OBJS) $(LIBS)

clean:
	rm -f $(LIB) $(OBJS)

clobber: clean
	rm -f .depend

depend:
	mkdep $(CFLAGS) $(ICISRCS)

install: $(LIBINSTALLS)
	$(INSTALL) $(LIBINSTALLS) $(PREFIX)/lib/ici4

xmlparse.o: expat/xmlparse/xmlparse.c
	$(CC) $(CFLAGS) -c expat/xmlparse/xmlparse.c

xmltok.o: expat/xmltok/xmltok.c
	$(CC) $(CFLAGS) -c expat/xmltok/xmltok.c

xmlrole.o: expat/xmltok/xmlrole.c
	$(CC) $(CFLAGS) -c expat/xmltok/xmlrole.c

$(OTHERRULES)
